{% assign hiddenProductTypes = settings.hidden_product_types | split: ',' %}
{% assign redirectToProduct = product.metafields.custom.redirect_to_product.value.url %}
{% assign redirectToURL = product.metafields.custom.redirect_to_url.value.url %}
{% assign hideProduct = 0 %}
{% assign disableRedirectLogic = product.metafields.custom.disable_all_redirect_logic %}

{% if disableRedirectLogic != true %}
  {% if product.price == 0 %}
    {% assign hideProduct = 1 %}
  {% else %}
    {% if hiddenProductTypes.size > 0 %}
      {% for hiddenProductType in hiddenProductTypes %}
        {% if product.type == hiddenProductType %}
          {% assign hideProduct = 1 %}
          {% break %}
        {% endif %}
      {% endfor %}
    {% endif %}
  {% endif %}
  
  {% if redirectToProduct or redirectToURL or hideProduct == 1 %}  
    {% if redirectToProduct %}
      {% assign redirectProductUrl = redirectToProduct %}
      {% comment %} Redirect to the Product PDP {% endcomment %}
      <script>
        window.location.href = "{{ redirectProductUrl }}";
      </script>   
    {% elsif redirectToURL %}
      {% assign redirectProductUrl = redirectToURL %}
      {% comment %} Redirect to the specified URL {% endcomment %}
      <script>
        window.location.href = "{{ redirectProductUrl }}";
      </script>   
    {% else %}
      {% comment %} Redirect to the Homepage {% endcomment %}
      <script>
        window.location = "/"
      </script>
    {% endif %}
  {% endif %}
{% endif %}
  
{%- comment -%} /sections/product-template {%- endcomment -%}
{%- liquid
  assign bg_color = section.settings.bg_color
  assign section_width = section.settings.width
-%}

{%- style -%}
  #ProductTemplate--{{ section.id }} {
    --PT: {{ section.settings.padding_top }}px;
    --PB: {{ section.settings.padding_bottom }}px;
  }

  {%- if bg_color != 'rgba(0,0,0,0)' and bg_color != '' -%}
    #ProductTemplate--{{ section.id }} { background: {{ bg_color }}; }
    .shop-pay-terms { --payment-terms-background-color: {{ bg_color }} }
  {%- endif -%}
{%- endstyle -%}

{%- if settings.site_brand == 'livingproof' and settings.site_region == 'us' -%}
  <link href="{{ 'product-lp.css' | asset_url }}" rel="stylesheet" type="text/css">
{% endif %}

<section id="ProductTemplate--{{ section.id }}"
  class="section-padding product-single{%- render 'button-fallback-classes', primary_color: settings.atc_button_color, secondary_color: settings.buynow_btn_color, section_bg_color: bg_color -%}"
  data-enable-history-state="true"
  data-section-type="product-template"
  data-section-id="{{ section.id }}"
  data-sticky-enabled="{{ section.settings.product_sticky_enable }}"
  data-tall-layout="{% if section.settings.image_layout == 'carousel' or section.settings.image_layout == 'carousel_only' %}false{% else %}true{% endif %}">
  <div class="{{ section_width }}">
    {%- render 'product' section: section, unique: section.id -%}
  </div>
</section>

{%- assign current_variant = product.selected_or_first_available_variant -%}
<script type="application/ld+json">
{
  "@context": "http://schema.org/",
  "@type": "Product",
  "name": {{ product.title | strip_html | json }},
  "url": {{ shop.url | append: product.url | json }},
  {%- if product.featured_media -%}
    {%- assign media_size = product.featured_media.preview_image.width | append: 'x' -%}
    "image": [
      {{ product.featured_media | img_url: media_size | prepend: "https:" | json }}
    ],
  {%- endif -%}
  "description": {{ product.description | strip_html | json }},
  {%- if current_variant.sku != blank -%}
    "sku": {{ current_variant.sku | json }},
  {%- endif -%}
  "brand": {
    "@type": "Thing",
    "name": {{ product.vendor | json }}
  },
  {%- if current_variant.barcode -%}
    "gtin8": "{{ product.variants.first.barcode }}",
  {%- endif -%}
  {%- if current_variant.barcode.size == 12 -%}
    "gtin12": "{{ product.variants.first.barcode }}",
  {%- endif -%}
  {%- if current_variant.barcode.size == 13 -%}
    "gtin13": "{{ product.variants.first.barcode }}",
  {%- endif -%}
  {%- if current_variant.barcode.size == 14 -%}
    "gtin14": "{{ product.variants.first.barcode }}",
  {%- endif -%}
  "offers": [
    {%- for variant in product.variants -%}
      {
        "@type" : "Offer",
        {%- if variant.sku != blank -%}
          "sku": {{ variant.sku | json }},
        {%- endif -%}
        "availability" : "http://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}",
        "price" : {{ variant.price | divided_by: 100.00 | json }},
        "priceCurrency" : {{ cart.currency.iso_code | json }},
        "url" : {{ shop.url | append: variant.url | json }}
      }{% unless forloop.last %},{% endunless %}
    {%- endfor -%}
  ]
}
</script>

